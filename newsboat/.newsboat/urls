# Query feeds
"query:Favourite Articles:flags # \"s\""
"query:Today's Articles: age <= 0"
"query:Unread Articles (1 week old):unread = \"yes\"  and age between 0:7"
"query:Unread Articles (1 month old):unread = \"yes\" and age between 0:31"
"query:Unread Articles (1 year old):unread = \"yes\"  and age between 0:365"
"query:Unread Articles:unread = \"yes\""
"query:Fun:unread = \"yes\" and tags # \"fun\""


